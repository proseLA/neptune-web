{
  "name": "@transferwise/components",
  "version": "39.11.4",
  "description": "Neptune React components",
  "license": "Apache-2.0",
  "main": "./build/umd/polyfill/main.js",
  "module": "./build/es/polyfill/index.js",
  "types": "./build/types/index.d.ts",
  "style": "./build/main.css",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "build",
    "scripts",
    "upgrades"
  ],
  "repository": {
    "type": "git",
    "fullname": "transferwise/neptune",
    "url": "git+https://github.com/transferwise/neptune-web.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:watch-less": "gulp watchLess --dest=src",
    "dev:storybook": "start-storybook -p 3001",
    "dev:translations": "npx npm-watch",
    "build": "npm-run-all build:*",
    "build:clean": "rm -rf lib build",
    "build:crowdin-source-file": "formatjs extract 'src/**/*.messages.+(js|ts|tsx)' --out-file src/i18n/en.json --format simple && prettier --find-config-path --write src/i18n/*.json",
    "build:compile-css": "gulp compileLess --dest=src",
    "build:es": "NODE_ENV=es babel src --extensions '.tsx,.js,.ts' -d build/es/polyfill --ignore '**/*.spec.js','**/*.story.js','**/__mocks__','**/test-utils'",
    "build:es-nopolyfill": "NODE_ENV=es-nopolyfill babel src --extensions '.tsx,.js,.ts' -d build/es/no-polyfill --ignore '**/*.spec.js','**/*.story.js','**/__mocks__','**/test-utils'",
    "build:umd": "NODE_ENV=umd rollup -c",
    "build:umd-nopolyfill": "NODE_ENV=umd-nopolyfill rollup -c",
    "build:generate-types": "tsc && node ./scripts/generate-type-declarations.js",
    "build:copy-files": "cpx 'src/**/!(db)/*.{json,svg}' build/es/polyfill & cpx 'src/**/!(db)/*.{json,svg}' build/es/no-polyfill",
    "build:copy-css": "cpx 'src/main.css' build/ & cpx 'src/**/*.css' build/styles/",
    "build:copy-lang": "cpx 'src/i18n/*.json' build/i18n && cpx 'src/i18n/index.js' build/i18n",
    "docs": "yarn build",
    "test": "jest --env=jsdom --maxWorkers=4 --config=../../node_modules/@transferwise/test-config/jest.config.js",
    "test:watch": "jest --watch --env=jsdom --config=../../node_modules/@transferwise/test-config/jest.config.js",
    "lint": "jest --maxWorkers=4 --config ../../node_modules/@transferwise/test-config/test/jest.lint.js && yarn lintless",
    "test:visual": "build-storybook && percy storybook ./storybook-static",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lintless": "stylelint --allow-empty-input './src/**/*.less'",
    "lintless:fix": "yarn lintless --fix"
  },
  "bin": {
    "neptune-upgrade-util": "scripts/neptune-upgrader.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@formatjs/cli": "^4.2.31",
    "@percy/cli": "1.0.0-beta.63",
    "@percy/storybook": "~4.0.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@transferwise/icons": "^2.12.0",
    "@transferwise/less-config": "^3.0.2",
    "@transferwise/neptune-css": "^12.3.8",
    "@transferwise/neptune-tokens": "7.0.0",
    "@transferwise/test-config": "^5.0.0",
    "@types/jest": "^26.0.20",
    "@types/jscodeshift": "^0.11.0",
    "@types/node": "^14.14.31",
    "@types/react-transition-group": "4.4.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-intl": "^8.2.16",
    "babel-preset-minify": "^0.5.1",
    "html-loader": "^1.3.2",
    "lodash.times": "^4.3.2",
    "npm-watch": "^0.11.0",
    "react-intl": "^5.10.0",
    "react-to-typescript-definitions": "~3.0.1",
    "rollup": "^2.34.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@transferwise/icons": "^2.12.0",
    "@transferwise/neptune-css": "^12",
    "currency-flags": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.14 || ^17",
    "react-dom": "^16.14 || ^17",
    "react-intl": "^5.10.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "@formatjs/intl-locale": "^2.4.14",
    "@popperjs/core": "^2.6.0",
    "@transferwise/formatting": "^2.1.0",
    "@transferwise/neptune-validation": "^3.0.4",
    "@types/react-transition-group": "4.4.4",
    "classnames": "^2.2.6",
    "commonmark": "0.29.1",
    "core-js": "^3.8.0",
    "inquirer": "^8.1.1",
    "inquirer-file-tree-selection-prompt": "^1.0.7",
    "jscodeshift": "^0.11.0",
    "lodash.clamp": "^4.0.3",
    "lodash.debounce": "^4.0.8",
    "lodash.difference": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "lodash.topairs": "^4.3.0",
    "react-popper": "^2.2.4",
    "react-required-if": "^1.0.3",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.4.2"
  },
  "watch": {
    "build:crowdin-source-file": "src/**/*.messages.js"
  },
  "resolutions": {
    "@storybook/**/kind-of": "^6.0.3"
  }
}
