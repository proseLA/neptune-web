{
  "name": "@transferwise/neptune-docs",
  "version": "12.5.6",
  "description": "Neptune Web",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:compile-css": "gulp watchLess --src='static/assets' --dest='static/assets'",
    "dev:next": "node ./docgen.js && next dev",
    "docs": "yarn dev",
    "build": "npm-run-all build:*",
    "build:compile-css": "gulp compileLess --src='static/assets' --dest='static/assets'",
    "build:docs-gen": "node ./docgen.js && node ./utils/bundleSize/getBundleSize.js",
    "build:next": "next build",
    "docs:deploy": "npm-run-all deploy:*",
    "deploy:export": "next export -o docs-build && touch docs-build/.nojekyll",
    "deploy:gh-pages": "deploy-to-github-pages -d docs-build -m main",
    "lint": "jest --maxWorkers=4 --config ../../node_modules/@transferwise/test-config/test/jest.lint.js",
    "lint:fix": "eslint components/ pages/ utils/ --fix"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/neptune-web",
    "url": "git+https://github.com/transferwise/neptune-web.git"
  },
  "keywords": [],
  "author": {
    "name": "Wise",
    "url": "https://wise.com/"
  },
  "bugs": {
    "url": "https://github.com/transferwise/neptune-web/issues"
  },
  "homepage": "https://transferwise.github.io/neptune-web",
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@mapbox/rehype-prism": "^0.7.0",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^11.0.1",
    "@transferwise/components": "^36.13.0",
    "@transferwise/dynamic-flows": "^14.0.4",
    "@transferwise/icons": "^2.14.0",
    "@transferwise/neptune-css": "^9.2.2",
    "@transferwise/test-config": "^3.0.7",
    "@zeit/next-css": "^1.0.1",
    "bootstrap": "github:transferwise/bootstrap#semver:^5.20.0",
    "classnames": "^2.3.1",
    "currency-flags": "^3.2.1",
    "date-fns": "^2.23.0",
    "glob": "^7.1.7",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "next": "^11.0.1",
    "next-images": "^1.8.1",
    "next-transpile-modules": "^8.0.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react-docgen": "^5.4.0",
    "react-docgen-external-proptypes-handler": "^2.0.0",
    "react-intl": "^5.20.6",
    "react-live": "^2.2.3",
    "react-required-if": "^1.0.3",
    "rehype-slug": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@transferwise/less-config": "^3.0.2",
    "@types/classnames": "^2.3.1",
    "@types/node": "^16.4.7",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "deploy-to-github-pages": "1.0.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^16.14 || ^17",
    "react-dom": "^16.14 || ^17"
  },
  "resolutions": {
    "lint-staged/**/kind-of": "^6.0.3",
    "next/**/kind-of": "^6.0.3"
  }
}
