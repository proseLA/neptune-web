{
  "extends": ["@transferwise", "../../.eslintrc"],
  "ignorePatterns": ["liveEditorCode/*.code.*"],
  "rules": {
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": ["Link"],
        "specialLink": ["hrefLeft", "hrefRight"],
        "aspects": ["invalidHref", "preferButton"]
      }
    ],
    "react/jsx-max-depth": [0]
  }
}
